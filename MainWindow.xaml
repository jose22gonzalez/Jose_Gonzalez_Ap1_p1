<Window x:Class="Jose_Gonzalez_Ap1_p1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jose_Gonzalez_Ap1_p1"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
         <StackPanel> 
            <Menu> 
                <MenuItem Header="Registros" >
                    <MenuItem Header="Productos" x:Name="rProductos" Click ="RegistroMenuItem_Click" /> 
                </MenuItem>

                <MenuItem Header="Consultas" >
                    <MenuItem Header="consulta" x:Name="rConsulta" Click ="ConsultaMenuItem_Click"/>
                </MenuItem>
            </Menu>

        </StackPanel>
    </Grid>
</Window>
